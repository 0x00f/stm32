./configure --host=arm-none-eabi --prefix=/home/iztok/stm32/lib/cpputest LDFLAGS='--specs=nosys.specs -mcpu=cortex-m4 -mthumb -mlittle-endian -mfpu=fpv4-sp-d16 -mfloat-abi=hard -mthumb-interwork' CFLAGS='-mcpu=cortex-m4 -mthumb -mlittle-endian -mfpu=fpv4-sp-d16 -mfloat-abi=hard -mthumb-interwork -std=c99' CXXFLAGS='-mcpu=cortex-m4 -mthumb -mlittle-endian -mfpu=fpv4-sp-d16 -mfloat-abi=hard -mthumb-interwork -std=c++11'
# Remove C+11 flag
# Remove C99 flag

make
make install
